{
  "name": "basic-react-boilerplate",
  "version": "1.0.0",
  "description": "basic-react-boilerplate for fast start with front end development",
  "main": "index.js",
  "scripts": {
    "build": "npm run precommit && webpack --config webpack.production.babel.js",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "lint:precommit": "lint-staged",
    "precommit": "npm run lint:precommit && npm test",
    "start": "webpack-dev-server --config ./webpack.development.babel.js",
    "test": "jest --passWithNoTests"
  },
  "keywords": [
    "js",
    "es6",
    "boilerplate",
    "react",
    "redux",
    "react-saga",
    "webpack"
  ],
  "author": "Sergeev Sergey",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^0.1.17",
    "css-loader": "^1.0.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "jest": "^23.6.0",
    "lint-staged": "^8.1.5",
    "mini-css-extract-plugin": "^0.4.5",
    "regenerator-runtime": "^0.12.1",
    "resolve-url-loader": "^3.1.0",
    "style-loader": "^0.23.0",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.3.1",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "@babel/runtime": "^7.4.3",
    "normalize.css": "^8.0.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  }
}
