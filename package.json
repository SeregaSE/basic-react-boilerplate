{
  "name": "basic-react-boilerplate",
  "version": "1.0.0",
  "description": "bootstrap react app fast and easy",
  "main": "src/index.js",
  "scripts": {
    "start": "NODE_ENV=${NODE_ENV=development} PORT=${PORT=3000} webpack-dev-server --config ./webpack/development.js",
    "lint": "eslint --ext .js ./",
    "lint:fix": "eslint --fix --ext .js ./",
    "build": "npm run lint:fix && npm test && NODE_ENV=${NODE_ENV=production} webpack --config ./webpack/production.js",
    "test": "jest --passWithNoTests ./test"
  },
  "keywords": [
    "react",
    "boilerplate",
    "webpack"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/SeregaSE/basic-react-boilerplate"
  },
  "author": "Sergey Sergeyev",
  "license": "MIT",
  "dependencies": {
    "normalize.css": "^8.0.1",
    "prop-types": "^15.7.2",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-router-dom": "^5.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/node": "^7.6.3",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "autoprefixer": "^9.7.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "clean-webpack-plugin": "^2.0.2",
    "copy-webpack-plugin": "^5.0.4",
    "css-loader": "^2.1.1",
    "cssnano": "^4.1.10",
    "eslint": "^6.6.0",
    "eslint-plugin-react": "^7.16.0",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "mini-css-extract-plugin": "^0.7.0",
    "postcss-loader": "^3.0.0",
    "postcss-modules-values": "^3.0.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.9",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-dev-server": "^3.9.0",
    "webpack-merge": "^4.2.2"
  }
}
