{
  "name": "basic-react-boilerplate",
  "version": "1.0.0",
  "description": "basic-react-boilerplate for fast start with front end development",
  "main": "index.js",
  "scripts": {
    "build": "npm run precommit && webpack --config webpack.production.babel.js",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "lint:precommit": "lint-staged",
    "precommit": "npm run lint:precommit && npm test",
    "start": "webpack-dev-server --config ./webpack.development.babel.js",
    "test": "jest --passWithNoTests"
  },
  "keywords": [
    "js",
    "es6",
    "boilerplate",
    "react",
    "redux",
    "react-saga",
    "webpack"
  ],
  "author": "Sergeev Sergey",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^8.0.2",
    "babel-jest": "^23.0.1",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "clean-webpack-plugin": "^0.1.17",
    "css-loader": "^0.28.4",
    "eslint": "^4.8.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.4.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "jest": "^23.1.0",
    "lint-staged": "^7.2.0",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.5.3",
    "react-hot-loader": "^4.3.0",
    "redux-devtools-extension": "^2.13.2",
    "regenerator-runtime": "^0.11.1",
    "resolve-url-loader": "^2.1.1",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.3",
    "webpack-dev-server": "^3.1.4",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "normalize.css": "^8.0.0",
    "prop-types": "^15.6.1",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-redux": "^5.0.6",
    "redux": "^4.0.0",
    "redux-saga": "^0.16.0",
    "styled-components": "^3.3.2"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  }
}
