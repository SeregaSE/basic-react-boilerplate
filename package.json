{
  "name": "basic-react-boilerplate",
  "version": "1.0.0",
  "description": "bootstrap react app fast and easy",
  "main": "src/index.js",
  "scripts": {
    "start": "NODE_ENV=${NODE_ENV=development} PORT=${PORT=3000} webpack-dev-server --config ./webpack/development.js",
    "lint": "eslint --ext .js ./",
    "lint:fix": "eslint --fix --ext .js ./",
    "build": "NODE_ENV=${NODE_ENV=production} npm run lint:fix && npm test && webpack --config ./webpack/production.js",
    "test": "jest --passWithNoTests ./test"
  },
  "keywords": [
    "react",
    "boilerplate",
    "webpack",
    "typescript"
  ],
  "author": "Sergey Sergeyev",
  "license": "MIT",
  "dependencies": {
    "normalize.css": "^8.0.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/node": "^7.2.2",
    "@babel/plugin-transform-runtime": "^7.5.0",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.6.1",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "clean-webpack-plugin": "^2.0.2",
    "copy-webpack-plugin": "^5.0.3",
    "css-loader": "^2.1.1",
    "eslint": "^6.0.1",
    "eslint-plugin-react": "^7.14.2",
    "file-loader": "^4.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.8.0",
    "mini-css-extract-plugin": "^0.7.0",
    "postcss-loader": "^3.0.0",
    "postcss-modules-values": "^3.0.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.31.0",
    "webpack-cli": "^3.3.5",
    "webpack-dev-middleware": "^3.7.0",
    "webpack-dev-server": "^3.7.2",
    "webpack-merge": "^4.2.1"
  }
}
